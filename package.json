{
  "name": "@percy/puppeteer",
  "description": "Pupppeteer client library for visual testing with Percy",
  "version": "2.0.2",
  "license": "MIT",
  "author": "Perceptual Inc.",
  "repository": "https://github.com/percy/percy-puppeteer",
  "keywords": [
    "puppeteer",
    "percy",
    "visual testing"
  ],
  "main": "index.js",
  "types": "types/index.d.ts",
